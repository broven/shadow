{
    "manifest_version": 2,
  "name": "shadow",
  "version": "0.0.1",
  "description": "track your time",
  "browser_action": {
  "default_title": "shadow",      // optional; shown in tooltip
  "default_popup": "popup.html"        // optional
  },
  "author": "metaaccount@qq.com",
  "content_scripts": [
    {
      "matches":["*://*/*"],
      "js": ["main.js"]
    }
  ],
  "permissions": [
      "storage"
    ]
}